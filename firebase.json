{
  "hosting": {
    "public": "/",
    "headers": [
      {
        "source": "**/*.@(jpg|jpeg|gif|png|svg)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=3600000"
          }
        ]
      }
    ],
    "ignore": [
      "**/.*"
    ],
    "cleanUrls": true
  }
}
